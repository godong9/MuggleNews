<div class="muggle-preview-list">
    <% for (var i = 0; i < timelines.length; i++) { %>
    <% var timeline = timelines[i]; %>
    <a href="/timelines/<%= timeline.id %>">
        <span class="p-thum-img"><img <% if(timeline.cover_img) { %> class="has-cover" src="<%= timeline.cover_img %>" <% } %> alt=""></span>
        <div class="p-list-wrap">
            <div class="title">
                <p><%= timeline.title %></p>
            </div>
            <div class="desc">
                <p><%= timeline.subtitle %></p>
            </div>
        </div>
        <div class="last-edit">
            <p><span class="user-name">by. <%= timeline.user_name %></span><span>|</span><span><%= timeline.date_text %></span></p>
        </div>
        <div class="view-count">
            <span><%= timeline.view_count %></span>
        </div>
    </a>
    <% } %>
</div>